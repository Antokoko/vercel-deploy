{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/",
            "dest": "signup.html"
        },
        {
            "src": "css/styles.css",
            "dest": "public/css/styles.css"
        }
    ]
}